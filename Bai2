package B2;

import java.util.Scanner;

/**
 *
 * @author tranthu
 */
public class java {
    public static void main(String[] args) {
        System.out.println("Nhap chuoi: ");
        
        String string = "";
        while(string.length() < 8)
        {
            Scanner sc = new Scanner(System.in);
            string = sc.nextLine();
            
        } 
    }
}
